{
  "name": "adtech/templates",
  "description": "Manage Templates",
  "license": "MIT",
  "authors": [
    {
      "name": "electric",
      "email": "ninhnguyenhoang@vccorp.vn"
    }
  ],
  "require": {
    "php": ">=5.6.4",
    "illuminate/notifications": "^5.4",
    "illuminate/support": "^5.1|^5.2|^5.3|^5.4",
    "packages/adtech/application": "^1.0"
  },
  "require-dev": {
    "mockery/mockery": "^0.9.5",
    "phpunit/phpunit": "4.*"
  },
  "autoload": {
    "psr-4": {
      "Adtech\\Templates\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Adtech\\Templates\\Test\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "config": {
    "sort-packages": true
  }
}
